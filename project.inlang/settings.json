{
  "$schema": "https://inlang.com/schema/project-settings",
  "baseLocale": "en",
  "locales": [
    "en",
    "de"
  ],
  "modules": [
    "https://cdn.jsdelivr.net/npm/@inlang/plugin-message-format@4/dist/index.js",
    "https://cdn.jsdelivr.net/npm/@inlang/plugin-m-function-matcher@2/dist/index.js"
  ],
  "plugin.inlang.messageFormat": {
    "pathPattern": [
      "./messages/{locale}/about.json",
      "./messages/{locale}/app_installed.json",
      "./messages/{locale}/common.json",
      "./messages/{locale}/contact.json",
      "./messages/{locale}/cookies.json",
      "./messages/{locale}/data.json",
      "./messages/{locale}/donation.json",
      "./messages/{locale}/dashboard.json",
      "./messages/{locale}/feedback.json",
      "./messages/{locale}/faq.json",
      "./messages/{locale}/footer.json",
      "./messages/{locale}/home.json",
      "./messages/{locale}/imprint.json",
      "./messages/{locale}/layout.json",
      "./messages/{locale}/login.json",
      "./messages/{locale}/market_news.json",
      "./messages/{locale}/pricing.json",
      "./messages/{locale}/privacy.json",
      "./messages/{locale}/register.json",
      "./messages/{locale}/searchbar.json",
      "./messages/{locale}/sitemap.json",
      "./messages/{locale}/terms.json"
    ]
  }
}
